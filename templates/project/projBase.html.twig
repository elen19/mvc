<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <link rel="icon" href="{{ asset('img/glider.svg') }}">
        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        {% block stylesheets %}
            {{ encore_entry_link_tags('proj') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </head>
    <body>
        <header class="site-header" style="background-image: url({{ asset('img/casino.jpg') }})">
            <span class="site-title">My Project Site</span>
        </header>
        <nav>
            <ul>
                <li><a href="{{ path('proj') }}">Home</a></li>
                <li><a href="{{ path('proj_about') }}">About</a></li>
                <li><a href="{{ path('proj_reset') }}">Reset</a></li>
            </ul>
        </nav>
        {% block body %}{% endblock %}

        <footer>
            <p>Copyright pending &#169 by Elis Engstr√∂m</p>
        </footer>
    </body>
</html>
